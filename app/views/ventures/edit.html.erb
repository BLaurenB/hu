<h3 class="color-teal" id="size-1dot5-em">Edit <%= @venture.title %></h3>
<br>

<%= form_for @venture do |f| %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :description %>
<%= f.text_field :description %>
<br><br>
<h3>Select your Industry</h3>
<%= f.select(:category_id, @categories.map {|c| [c.title, c.id]}) %>
<br>
<br>
<br>
<%= f.label :notes %><br>
<%= f.text_area :notes %><br>
<%= f.submit %>

<% end  %>
